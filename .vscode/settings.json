{
  "search.exclude": {
    "**/node_modules": true,
    "npm-debug.log*": true,
    "**/static": true,
    "dist/": true,
    "yarn-error.*": true,
    "**/yarn.lock": true
  },
  "editor.formatOnSave": true,
  "editor.defaultFormatter": "esbenp.prettier-vscode",
  "[typescriptreact]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "[javascript]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  }
}
